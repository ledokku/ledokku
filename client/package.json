{
  "name": "ledokku-client",
  "private": true,
  "version": "0.4.0",
  "license": "MIT",
  "scripts": {
    "dev": "yarn watch:css && SKIP_PREFLIGHT_CHECK=true react-scripts start",
    "build": "yarn build:css && SKIP_PREFLIGHT_CHECK=true react-scripts build",
    "test": "react-scripts test",
    "build:css": "postcss src/styles/index.css -o src/generated/index.css",
    "watch:css": "postcss src/styles/index.css -o src/generated/index.css",
    "generate": "graphql-codegen --config codegen.yml"
  },
  "eslintConfig": {
    "extends": "react-app",
    "rules": {
      "react/react-in-jsx-scope": "off"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@apollo/client": "3.2.5",
    "@headlessui/react": "0.2.0",
    "subscriptions-transport-ws": "0.9.18",
    "ansi_up": "4.0.4",
    "classnames": "2.2.6",
    "formik": "2.2.1",
    "graphql": "15.3.0",
    "jwt-decode": "2.2.0",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-feather": "2.0.9",
    "react-router-dom": "5.2.0",
    "styled-components": "5.2.1",
    "react-toastify": "6.0.9",
    "yup": "0.31.0"
  },
  "devDependencies": {
    "@graphql-codegen/add": "2.0.1",
    "@graphql-codegen/cli": "1.18.0",
    "@graphql-codegen/typescript": "1.17.11",
    "@graphql-codegen/typescript-operations": "1.17.8",
    "@graphql-codegen/typescript-react-apollo": "2.0.7",
    "@tailwindcss/forms": "0.2.1",
    "@types/classnames": "2.2.10",
    "@types/jwt-decode": "2.2.1",
    "@types/react-toastify": "4.1.0",
    "@types/node": "14.14.2",
    "@types/react": "17.0.0",
    "@types/react-dom": "17.0.0",
    "@types/react-router-dom": "5.1.6",
    "@types/react-transition-group": "4.4.0",
    "@types/styled-components": "5.1.4",
    "@types/yup": "0.29.9",
    "autoprefixer": "10.0.2",
    "postcss": "8.1.9",
    "postcss-cli": "8.3.0",
    "react-scripts": "4.0.1",
    "tailwindcss": "2.0.1",
    "typescript": "4.1.2"
  }
}
