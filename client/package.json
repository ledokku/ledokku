{
  "name": "ledokku-client",
  "private": true,
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "dev": "npm run watch:css && SKIP_PREFLIGHT_CHECK=true react-scripts start",
    "build": "npm run build:css && SKIP_PREFLIGHT_CHECK=true react-scripts build",
    "test": "react-scripts test",
    "build:css": "postcss src/styles/index.css -o src/generated/index.css",
    "watch:css": "postcss src/styles/index.css -o src/generated/index.css",
    "generate": "graphql-codegen --config codegen.yml"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@apollo/react-hooks": "3.1.5",
    "apollo-cache-inmemory": "1.6.6",
    "apollo-client": "2.6.10",
    "apollo-link-context": "1.0.20",
    "apollo-link-error": "1.1.13",
    "apollo-link-http": "1.5.17",
    "classnames": "2.2.6",
    "formik": "2.1.4",
    "graphql": "15.1.0",
    "jwt-decode": "2.2.0",
    "polished": "3.6.3",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-feather": "2.0.8",
    "react-router-dom": "5.2.0",
    "react-transition-group": "4.4.1",
    "socket.io-client": "2.3.0",
    "styled-components": "5.1.1",
    "yup": "0.29.1"
  },
  "devDependencies": {
    "@graphql-codegen/add": "1.13.5",
    "@graphql-codegen/cli": "1.13.5",
    "@graphql-codegen/typescript": "1.13.5",
    "@graphql-codegen/typescript-operations": "1.13.5",
    "@graphql-codegen/typescript-react-apollo": "1.13.5",
    "@tailwindcss/ui": "0.3.0",
    "@types/classnames": "2.2.10",
    "@types/jwt-decode": "2.2.1",
    "@types/node": "14.0.1",
    "@types/react": "16.9.35",
    "@types/react-dom": "16.9.8",
    "@types/react-router-dom": "5.1.5",
    "@types/react-transition-group": "4.4.0",
    "@types/socket.io-client": "1.4.33",
    "@types/styled-components": "5.1.0",
    "@types/yup": "0.29.3",
    "postcss-cli": "7.1.1",
    "postcss-preset-env": "6.7.0",
    "react-scripts": "3.4.1",
    "tailwindcss": "1.4.6",
    "typescript": "3.9.2"
  }
}
