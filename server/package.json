{
  "name": "ledokku-server",
  "private": true,
  "version": "0.4.0",
  "license": "MIT",
  "scripts": {
    "dev": "yarn prisma:migrate && yarn prisma:generate && nodemon 'src/index.ts' --exec 'ts-node' src/index.ts -e ts,graphql",
    "build": "tsc",
    "start": "yarn prisma:migrate && node build/index.js",
    "generate": "graphql-codegen --config codegen.yml",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate up --experimental",
    "prisma:migrate:save": "prisma migrate save --experimental"
  },
  "dependencies": {
    "@octokit/rest": "18.0.6",
    "@prisma/cli": "2.10.2",
    "@prisma/client": "2.10.2",
    "apollo-server": "2.18.2",
    "apollo-server-express": "2.18.2",
    "bullmq": "1.9.0",
    "debug": "4.2.0",
    "dotenv": "8.2.0",
    "execa": "4.0.3",
    "express": "4.17.1",
    "graphql": "15.3.0",
    "graphql-scalars": "1.4.0",
    "jsonwebtoken": "8.5.1",
    "node-fetch": "2.6.1",
    "node-ssh": "11.1.1",
    "sshpk": "1.16.1",
    "yup": "0.31.0"
  },
  "devDependencies": {
    "@graphql-codegen/add": "2.0.1",
    "@graphql-codegen/cli": "1.18.0",
    "@graphql-codegen/typescript": "1.17.11",
    "@graphql-codegen/typescript-resolvers": "1.17.10",
    "@types/debug": "4.1.5",
    "@types/jsonwebtoken": "8.5.0",
    "@types/node": "14.14.2",
    "@types/node-ssh": "7.0.0",
    "@types/yup": "0.29.9",
    "nodemon": "2.0.6",
    "ts-node": "9.0.0",
    "typescript": "4.1.2"
  }
}
