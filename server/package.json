{
  "name": "ledokku-server",
  "private": true,
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "dev": "yarn prisma:migrate && yarn prisma:generate && nodemon 'src/index.ts' --exec 'ts-node' src/index.ts -e ts,graphql",
    "build": "tsc",
    "start": "node .build/index.js",
    "generate": "graphql-codegen --config codegen.yml",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate up --experimental"
  },
  "dependencies": {
    "@octokit/rest": "17.9.2",
    "@prisma/client": "2.0.0-beta.5",
    "apollo-server": "2.13.1",
    "apollo-server-express": "2.13.1",
    "bullmq": "1.8.7",
    "debug": "4.1.1",
    "dotenv": "8.2.0",
    "execa": "4.0.1",
    "express": "4.17.1",
    "graphql": "15.0.0",
    "jsonwebtoken": "8.5.1",
    "node-fetch": "2.6.0",
    "node-ssh": "10.0.1",
    "socket.io": "2.3.0",
    "sshpk": "1.16.1",
    "yup": "0.28.5"
  },
  "devDependencies": {
    "@graphql-codegen/add": "1.13.5",
    "@graphql-codegen/cli": "1.13.5",
    "@graphql-codegen/typescript": "1.13.5",
    "@graphql-codegen/typescript-resolvers": "1.13.5",
    "@prisma/cli": "2.0.0-beta.5",
    "@types/debug": "4.1.5",
    "@types/jsonwebtoken": "8.5.0",
    "@types/node-ssh": "7.0.0",
    "@types/socket.io": "2.1.6",
    "@types/yup": "0.28.3",
    "nodemon": "2.0.4",
    "ts-node": "8.10.1",
    "typescript": "3.9.2"
  }
}
