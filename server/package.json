{
  "name": "ledokku-server",
  "private": true,
  "version": "0.6.0",
  "license": "MIT",
  "scripts": {
    "dev": "yarn prisma:migrate && yarn prisma:generate && nodemon 'src/index.ts' --exec 'ts-node' src/index.ts -e ts,graphql",
    "build": "tsc",
    "start": "yarn prisma:migrate && node build/index.js",
    "generate": "graphql-codegen --config codegen.yml",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate deploy --preview-feature",
    "prisma:migrate:save": "prisma migrate dev --preview-feature"
  },
  "dependencies": {
    "@octokit/rest": "18.3.0",
    "@prisma/client": "2.17.0",
    "apollo-server": "2.21.0",
    "apollo-server-express": "2.21.0",
    "bullmq": "1.9.0",
    "debug": "4.3.1",
    "dotenv": "8.2.0",
    "execa": "5.0.0",
    "express": "4.17.1",
    "graphql": "15.5.0",
    "graphql-scalars": "1.8.0",
    "jsonwebtoken": "8.5.1",
    "node-fetch": "2.6.1",
    "node-ssh": "11.1.1",
    "prisma": "2.17.0",
    "smee-client": "1.2.2",
    "sshpk": "1.16.1",
    "tslib": "2.1.0",
    "yup": "0.31.0"
  },
  "devDependencies": {
    "@graphql-codegen/add": "2.0.2",
    "@graphql-codegen/cli": "1.21.1",
    "@graphql-codegen/typescript": "1.21.0",
    "@graphql-codegen/typescript-resolvers": "1.18.2",
    "@types/debug": "4.1.5",
    "@types/jsonwebtoken": "8.5.0",
    "@types/node": "14.14.2",
    "@types/node-ssh": "7.0.0",
    "@types/yup": "0.29.9",
    "nodemon": "2.0.7",
    "smee-client-patched": "1.0.1",
    "ts-node": "9.1.1",
    "typescript": "4.2.2"
  }
}
